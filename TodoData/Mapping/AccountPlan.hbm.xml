<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly ="TodoData" namespace ="TodoData.Models.User" >
  <class name="AccountPlan" table="dbo.AccountPlans" >
    <id name ="Id" column ="bintId" type ="Int32" generator="native" />
    <property name="Name" column="vchName" type="string"/>
    <property name="Description" column="vchDescription" type="string" />
    <property name="IsChargeable" column="isChargeable" type="Boolean" />
    <property name="Amount" column="dcmAmount" type="Double" />
    <property name="Currency" column="intCurrency" type="Int32" />
    <property name="LastUpdate" column="dtLastUpdate" type="DateTime" />
  </class>
</hibernate-mapping>